<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape
            android:shape="oval">

            <gradient
                android:startColor="@color/laranja" 
                android:endColor="@color/laranja"
                android:angle="270" />
            <stroke
                android:width="3dp"
                />
            <corners
                android:radius="25dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
       <shape
           android:shape="oval">
            <gradient
                android:startColor="@color/laranjaClaro" 
                android:endColor="@color/laranjaClaro"
                android:angle="270" />
            <stroke
                android:width="3dp"
                /> 
            <corners
                android:radius="25dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    
   
</selector>
